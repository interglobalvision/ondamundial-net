{
  "name": "globie-wordpress-starter-pack",
  "description": "so I heard you wanna make a wordpress site",
  "main": "gulpfile.js",
  "authors": [
    "interglobalvision"
  ],
  "license": "ISC",
  "homepage": "https://github.com/interglobalvision/globie-wordpress-starter-pack",
  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components",
    "test",
    "tests"
  ],
  "dependencies": {
    "lazysizes": "^2.0.4",
    "moment": "^2.18.1",
    "three.js": "threejs#*",
    "mobile-detect": "hgoebl/mobile-detect.js#^1.3.6",
    "vue": "^2.5.2",
    "howler.js": "howler#^2.0.5"
  },
  "overrides": {
    "vue": {
      "main": [
        "dist/vue.js"
      ]
    },
    "moment": {
      "main": [
        "moment.js",
        "locale/es.js"
      ]
    },
    "three.js": {
      "main": [
        "build/three.js",
        "examples/js/renderers/CanvasRenderer.js",
        "examples/js/renderers/Projector.js"
      ]
    }
  }
}
